{
  "name": "graphql",
  "version": "0.0.1",
  "description": "Shaun's GQL Server",
  "main": "index.ts",
  "prisma": {
    "seed": " ts-node src/tests/seed.ts"
  },
  "scripts": {
    "lint": "yarn run eslint . --ext .graphql,.ts,.tsx",
    "test:setup": "dotenv -e .env.test -- npx prisma migrate reset --force",
    "test:run": "dotenv -e .env.test -- jest --runInBand --coverage",
    "test": "yarn test:setup && yarn test:run",
    "start": "ts-node-dev --files src/index.ts",
    "generate": "graphql-codegen",
    "dev": "concurrently \"ts-node-dev --files src/index.ts\" \"yarn generate --watch\"",
    "prepare": "husky install"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@graphql-tools/load-files": "^6.5.3",
    "@graphql-tools/merge": "^8.2.2",
    "@graphql-tools/schema": "^8.3.1",
    "@prisma/client": "^3.10.0",
    "apollo-server-core": "^3.6.4",
    "apollo-server-express": "^3.6.4",
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dayjs": "^1.10.8",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "graphql": "^16.3.0",
    "jsonwebtoken": "^8.5.1",
    "prisma": "^3.10.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.4.5",
    "@graphql-codegen/typescript-operations": "^2.3.5",
    "@graphql-codegen/typescript-resolvers": "^2.5.2",
    "@graphql-eslint/eslint-plugin": "^3.9.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.20",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "jest": "^27.5.1",
    "jest-mock-extended": "^2.0.4",
    "lint-staged": "11.2.6",
    "node-mocks-http": "^1.11.0",
    "prettier": "^2.6.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.2"
  }
}
